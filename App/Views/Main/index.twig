{% extends 'App/main.twig' %}

{% block current_data_title %}
    {{ currentLinkData.month }}/{{ currentLinkData.year }}
{% endblock %}

{% block previous_button %}
	<a href="{{  siteUrl(urlFor('main', previousLinkData)) }}" class="ui button">
		{{ previousLinkData.month }}/{{ previousLinkData.year }}
	</a>
{% endblock %}

{% block next_button %}
	<a href="{{  siteUrl(urlFor('main', nextLinkData)) }}" class="ui button">
		{{ nextLinkData.month }}/{{ nextLinkData.year }}
	</a>
{% endblock %}